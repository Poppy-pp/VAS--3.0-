import insuranceCountChart from '@/components/insuranceCountChart'
import { getInsuranceDataCount, getPolicyno, getCountByDay, getCountByMonth, getCountByQuarter, getCountByYear } from './service'
import { mapState } from 'vuex'

export default {
    name: "insuranceCount",
    data () {
        return {
            policynos: [],
            policyno: '',
            countData: {}
        }
    },
    components: {
        insuranceCountChart
    },
    computed: {
        ...mapState({
            corporateinfo: state=> {
                return state.user.corporateinfo
            }
        })
    },
    methods: {
        async getInsuranceDataCount () {
            try {
                const params = {
                    insurancecorpid: this.corporateinfo.id,
                    policyno: this.policyno
                }
                const {data} = await getInsuranceDataCount(params)
                this.countData = data.data
            } catch (e) {

            }
        },
        async getPolicyno () {
            try {
                const params = {
                    insurancecorpid: this.corporateinfo.id
                }
                const {data} = await getPolicyno(params)
                this.policynos = data.data
            } catch (e) {

            }
        }
    },
    mounted () {
        this.getInsuranceDataCount();
        this.getPolicyno();
    }
}