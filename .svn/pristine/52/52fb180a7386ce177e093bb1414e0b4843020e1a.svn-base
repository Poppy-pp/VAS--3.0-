				},

				addFormVisible: false, //新增界面是否显示
				addLoading: false,
				addFormRules: {
					suppliercode:[{validator: checkSpecial,trigger: 'blur'}],
			                    	suppliername: [{
			                        		required: true,
			                        		message: '请输入供应商名称',
			                        		trigger: 'blur'
			                    	},{validator: checkChinese,trigger: 'blur'}],
			                    	suppliertype: [{
			                        		required: true,
			                        		message: '请选择供应商类别',
			                        		trigger: 'blur'
			                    	}],
			                    	linkman: [{validator: checkChinese,trigger: 'blur'}],
			                    	contactno: [{validator: checkMobile,trigger: 'blur'}],
				},
				//新增供应商界面数据
				addForm: {
				       suppliercode: '',
			                    suppliername: '',
			                    suppliertype: '',
			                    supplierlevel:'1',
			                    supplieraddress: '',
			                    addresslng:'',
			                    addresslat:'',
			                    linkman: '',
			                    contactno: '',
			                    remark: '',
				},
				// 类型设置
				typeFormVisible:false,//设备型号设置
				typeListData:[],//设备型号数据
				typeListLoading:false,
				typetotal:0,//设备类型分页
				typepageSize:15,
				typecurrentPage: 1,
				editable:false,//是否可编辑
				typeForm:{
					modelitem:'',
