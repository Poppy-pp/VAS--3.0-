server {
        listen       80;
        server_name  localhost;

        #charset koi8-r;

        #access_log  logs/host.access.log  main;

        location / {
            root   /usr/share/nginx/html;
            index  index.html index.htm;
        }
		
		location /admin/ {
            proxy_pass   http://192.168.50.150:9999/admin/;
		}
		
		location /insurance/ {
            proxy_pass   http://192.168.50.150:9999/insurance/;
		}
		
		location /admindept/ {
            proxy_pass   http://192.168.50.150:9999/admindept/;
		}
		
		location /auth/ {
            proxy_pass   http://192.168.50.150:9999/auth/;
		}
		
#		location /vasms-web/ {
#            proxy_pass   http://222.212.141.40:9104/vasms-web/;
#		}
		
		location /oauth/ {
            proxy_pass   http://openapi.qzone.qq.com/;
		}
		
		location /gisapiservice/ {
            proxy_pass   http://139.219.109.157:9204/gisapiservice/;
		}
		
		location /ueditor/ {
            proxy_pass   http://192.168.0.213:8080/vasms-web/;
		}
		
		location /iData/ {
            proxy_pass   http://test.wwvas.com/iData/;
		}
		
		location /jim/ {
            proxy_pass   http://139.219.98.197:2002/jim/;
		}
		
		location /img/ {
            proxy_pass   http://222.212.141.40:9104/img/;
        }
		
		location /image/ {
            proxy_pass   http://192.168.50.146/image/;
        }
		
		location /template/ {
            proxy_pass   http://127.0.0.1:9104/template/;
        }
		location /vasms-uche/ {
            proxy_pass   http://127.0.0.1:9911/vasms-uche/;
        }
		location /redis/ {
            proxy_pass   http://127.0.0.1:6379;
        }
		location /vasms-web/ {
            proxy_pass   http://222.212.141.40:9104;
			proxy_redirect    off;  
			proxy_set_header  Host             $host;  
			proxy_set_header  X-Real-IP        $remote_addr;  
			proxy_set_header  X-Forwarded-For  $proxy_add_x_forwarded_for;  
        }
		location /uflo/ {
            proxy_pass   http://127.0.0.1:9104/uflo/;
        }
#		location /img/ {
#            proxy_pass   http://127.0.0.1:9104/img/;
