            handleChange: function(index, row) {
                this.$confirm('确认设置该条记录的状态吗？', '提示', {
                    type: 'warning'
                }).then(() => {
                    let para = {
                        id: row.ID,
                        isactive: row.ISACTIVE == 0 ? 1 : 0,
                    }
                    modifyInsuranceInfo(para).then((res) => {
                        if (res.data.result.code ==0 ) {
                            this.$message({
                                message: '设置成功',
                                type: 'success'
                            });
                        }
                        row.ISACTIVE = para.isactive;
                    }).catch(() => {
                        this.listLoading = false;
                    });
