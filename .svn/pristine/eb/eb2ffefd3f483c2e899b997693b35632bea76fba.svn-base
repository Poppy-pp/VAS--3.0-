			//车型选择
			handleItemChange(queryString, cb) {
				let para = {
						model: queryString,
					            limit: 30,
					},
					ownerArray = [];
				getVehTypeList(para).then((res) => {
					if(res.data.data != null) {
						res.data.data.records.forEach(function(item, index) {
							ownerArray.push({
								value: item.model,
								brand: item.brand,
								serious: item.serious
							});
						});
						cb(ownerArray);
					}
				});
			},
			//选择车型查询颜色
			handleSelectColor(v) {
				this.tempModel = v.value;
				this.editForm.model = v.value;
				this.editForm.color = '';
				this.addForm.model = v.value;
				this.addForm.color = '';
				let para = {
					model: v.value
				};
				getAllColorList(para).then((res) => {
			                let carModelList = [];
			                res.data.data.forEach((res, index) => {
			                    if(res.color) {
			                        res.color.split(";").forEach((color, index) => {
			                            carModelList.push({
			                                color: color,
			                                color_rgb: res.color_rgb.split(";")[index]
			                            });
			                        });
			                    }
			                });
			                this.carColor = carModelList;
			            });
			},
